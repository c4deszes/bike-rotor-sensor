{
    "name": "test-network",
    "baudrate": 19200,
    "master": "BodyComputer",
    "nodes": {
        "BodyComputer": {
            "address": "0x0",
            "subscribes": [],
            "publishes": []
        },
        "RotorSensor": {
            "address": "0x1",
            "subscribes": [
            ],
            "publishes": [
                "SpeedStatus"
            ]
        }
    },
    "requests": {
        "SpeedStatus": {
            "id": "0x1000",
            "size": 5,
            "layout": {
                "Speed": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "GlobalSpeedEncoder"
                },
                "SpeedState": {
                    "offset": 16,
                    "width": 2,
                    "initial": "Error",
                    "encoder": "GlobalSpeedStateEncoder"
                },
                "FrontSpeedState": {
                    "offset": 18,
                    "width": 3,
                    "initial": "Off",
                    "encoder": "ChannelSpeedStateEncoder"
                },
                "RearSpeedState": {
                    "offset": 21,
                    "width": 3,
                    "initial": "Off",
                    "encoder": "ChannelSpeedStateEncoder"
                }
            }
        }
    },
    "encoders": {
        "GlobalSpeedEncoder": {
            "type": "formula",
            "scale": 0.1,
            "offset": 0.0,
            "unit": "km/h"
        },
        "GlobalSpeedStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "SlowResponse",
                "2": "Unreliable",
                "3": "Error"
            }
        },
        "ChannelSpeedStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "UNUSED",
                "2": "SensorWarning",
                "3": "SpeedUnreliable",
                "4": "SensorError",
                "5": "OutputOpen",
                "6": "OutputShorted",
                "7": "Off"
            }
        }
    },
    "schedules": {
        "Normal": {
            "rate": "30 ms",
            "strategy": "randomize|balance|order",
            "messages": {
                "DigitalRead": 1,
                "DigitalWrite": 1,
                "AnalogRead": 1
            }
        }
    }
}